{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "firstName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "lastName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "experience": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4
        },
        "education": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4
        },
        "frontend": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4
        },
        "backend": {
            "type": "integer",
            "minimum": 0,
            "maximum": 4
        }
    },
    "additionalProperties": false,
    "required": [
        "firstName",
        "lastName",
        "experience",
        "education",
        "frontend",
        "backend",
        "language"
    ]
}